  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bschneier/credit-cards-front-end"
  },
  "scripts": {
    "build": "SET NODE_ENV=production& npm-run-all eslint stylelint test compile",
    "compile": "rimraf dist/*& webpack --config config/webpack.prod.js",
    "prestart": "babel-node tools/startMessage.js",
    "start": "SET NODE_ENV=development& npm-run-all --parallel test:watch open:src eslint:watch stylelint",
    "open:src": "babel-node tools/srcServer.js",
    "stylelint": "stylelint --config config/stylelint.js --color src/stylesheets/*.scss",
    "eslint": "node_modules/.bin/esw --config config/eslint.js src tools",
    "eslint:watch": "npm run eslint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.23.0",
    "prop-types": "^15.5.10",
    "pubsub-js": "^1.5.7",

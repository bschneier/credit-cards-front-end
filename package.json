{
  "name": "credit-cards-front-end",
  "version": "1.0.0",
  "description": "Credit card rewards front end application",
  "author": "Brian Schneier",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bschneier/credit-cards-front-end"
  },
  "scripts": {
    "build": "SET NODE_ENV=production& npm-run-all check-security jslint stylelint test compile",
    "compile": "rimraf dist/*& webpack --config config/webpack.prod.js",
    "prestart": "npm-run-all --parallel check-security stylelint",
    "start": "SET NODE_ENV=development& npm-run-all --parallel test:watch jslint:watch open:dev-server",
    "open:dev-server": "webpack-dev-server --inline --hot --open --config config/webpack.dev.js",
    "stylelint": "stylelint --config config/stylelint.js --color src/stylesheets/*.scss",
    "jslint": "node_modules/.bin/esw --config config/eslint.js src tools",
    "jslint:watch": "npm run jslint -- --watch",
    "test": "jest --config=config/jest.config.js",
    "test:watch": "npm run test -- --watchAll",
    "check-security": "nsp check"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "pubsub-js": "^1.5.7",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.3",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "colors": "^1.1.2",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "nsp": "^2.8.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "1.0.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.1.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
